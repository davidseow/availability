{
  "name": "availability",
  "version": "1.0.0",
  "description": "Plan an event with friends, group of strangers the API-way!",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "format:check": "prettier --config .prettierrc 'src/**/*.ts' --check",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidseow/availability.git"
  },
  "keywords": [
    "scheduler",
    "planner"
  ],
  "author": "David Seow",
  "license": "none",
  "bugs": {
    "url": "https://github.com/davidseow/availability/issues"
  },
  "homepage": "https://github.com/davidseow/availability#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "mongodb": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.13",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "concurrently": "8.0.1",
    "eslint": "8.40.0",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
